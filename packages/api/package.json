{
  "name": "api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "eslint ./src --ext .js,.ts",
    "dev": "npm run lint && tsc && firebase emulators:start --import ./exported_for_local_development ",
    "build": "npm run lint && tsc",
    "serve": "export GOOGLE_APPLICATION_CREDENTIALS=\".serviceAccountKey.json\" && npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "firestore": "firebase deploy --only firestore",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "14"
  },
  "main": "lib/index.js",
  "dependencies": {
    "algoliasearch": "^4.10.5",
    "axios": "^0.21.4",
    "firebase-admin": "^9.8.0",
    "firebase-functions": "^3.14.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.7",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^4.0.0",
    "firebase-functions-test": "^0.2.0",
    "prettier": "^2.4.1",
    "typescript": "^3.8.0"
  }
}
